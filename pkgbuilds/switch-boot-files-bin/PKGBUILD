# Maintainer: Ezekiel Bethel <zek@9net.org>

pkgname=switch-boot-files-bin
pkgver=r32
pkgrel=3
epoch=
pkgdesc=""
arch=(any)
url=""
license=()
optdepends=()
install=
source=(https://9net.org/l4t-arch/boot-files-r32.tar.gz)
#source=(boot-files-r32.tar.gz)
sha256sums=(SKIP)
noextract=()
validpgpkeys=()

package() {
	mkdir -p $pkgdir/boot/l4t-arch/
	cp -r $srcdir/bootloader $pkgdir/boot/
	cp -r $srcdir/l4t-arch/{boot.scr,coreboot.rom} $pkgdir/boot/l4t-arch/
}
